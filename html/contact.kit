<!DOCTYPE html>
<html lang="pl">

<head>
    <meta charset="UTF-8">
    <meta http-equiv="X-UA-Compatible" content="IE=edge">
    <meta name="viewport" content="width=device-width, initial-scale=1.0">
    <title>Forest - kontakt</title>
    <!-- @include 'styles.kit' -->
</head>

<body>
    <!-- @include 'nav.kit' -->
    <!-- @include 'header-mob.kit' -->
    <!-- @include 'header.kit' -->

    <div class="contact">
        <div class="wrapper wrapper__contact">


            <div class="contact__box contact__contact-data">
                <i class="fa-solid fa-location-dot"></i>
                <h3>Adres:</h3>
                <p>Ul. Leśna 123</p>
                <p>41-940 Łodygowice</p>

                <i class="fa-solid fa-phone"></i>
                <h3>Numer telefonu:</h3>
                <p>+48 123-123-123</p>

                <i class="fa-regular fa-paper-plane"></i>
                <h3>Email:</h3>
                <p>contact@forestgroup.pl</p>
            </div>

            <div class="contact__box contact__map">
                <iframe
                    src="https://www.google.com/maps/embed?pb=!1m18!1m12!1m3!1d2559.846233644222!2d19.89240901593459!3d50.08916572121169!2m3!1f0!2f0!3f0!3m2!1i1024!2i768!4f13.1!3m3!1m2!1s0x47165a32728962db%3A0x4344067c5fa03761!2sJasnog%C3%B3rska%202%2C%2031-358%20Krak%C3%B3w!5e0!3m2!1spl!2spl!4v1675885006783!5m2!1spl!2spl"
                    allowfullscreen="" loading="lazy" referrerpolicy="no-referrer-when-downgrade"></iframe>
            </div>

            <div class="contact__box contact__form">
                <form action="https://formsubmit.co/dikta.e@gmail.com" method="POST">
                    <h2 class="contact__form-title">Formularz kontaktowy</h2>
                    <input name="mail" id="mail" type="email" placeholder="Twój adres email">
                    <p class="contact__form-error">Podaj poprawny adres email</p>
                    <textarea name="question" id="question" placeholder="Twoja wiadomość"></textarea>

                    <input type="text" name="_honey" style="display:none">
                    <input type="hidden" name="_next" value="https://evelina-di.github.io/forest-website/thankyou">

                    <p class="contact__form-info"></p>
                    <button type="submit" class="submit">Wyślij</button>
                </form>
            </div>
        </div>
    </div>
    </div>

    <!-- @include 'footer.kit' -->
    <script src="/forest-website/dist/js/main.min.js"></script>
    <!-- <script src="/dist/js/main.min.js"></script> -->
</body>

</html>